---
import Layout from '../layouts/Layout.astro';

const response = await fetch(
    "https://jsonplaceholder.typicode.com/posts/1"
)
const data = await response.json()
---

<!-- Astro uses file-based routing so this page is exposed at /example-request -->
<Layout title="Welcome to Astro.">
	<main class="bg-grey">
		<h1 class="text-white">This is an example GET request to https://jsonplaceholder.com.typicode.com/posts/1</h1>
        <p class="text-white">
			{JSON.stringify(data)}
		</p>
	</main>
</Layout>

